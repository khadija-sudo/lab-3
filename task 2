#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
    Node(int v):data(v),next(NULL){}
};
Node* mergeSortedLists(Node* a, Node* b) {
    if (!a) return b;
    if (!b) return a;
    Node* result = NULL;
    if (a->data <= b->data) {
        result = a;
        result->next = mergeSortedLists(a->next, b);
    } else {
        result = b;
        result->next = mergeSortedLists(a, b->next);
    }
    return result;
}
void printList(Node* head) {
    while (head) {
        cout << head->data << " ";
        head = head->next;
    }
}
int main() {
    Node* a = new Node(1);
    a->next = new Node(3);
    a->next->next = new Node(5);
    Node* b = new Node(2);
    b->next = new Node(4);
    b->next->next = new Node(6);
    Node* res = mergeSortedLists(a,b);
    printList(res);
}
